<script>
'use strict';

function supports(arr) {
    let pass = 0;
    let fail = 0;

    for(const obj of arr)
        CSS.supports(...obj.split(':')) ? pass++ : fail++;

    if(fail)
        console.error(`Incompatible browser (pass ${pass}, fail ${fail})`);
    else
        window.location.replace('/supported');
}

supports([
    'display: grid',
    'display: flex',
    'grid-template: "foo bar" 10px "baz qux" 1fr / 50% 1fr',
    'flex-direction: row-reverse',
    'flex-direction: column',
    'justify-content: center',
    'align-items: center',
    'height: 100vh',
    'height: calc(100% - 50px)'
]);
</script>

